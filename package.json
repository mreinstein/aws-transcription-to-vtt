{
  "name": "aws-transcription-to-vtt",
  "version": "1.0.3",
  "description": "convert an AWS transcribe JSON body into a .vtt file",
  "main": "dist/aws-transcription-to-vtt.cjs.js",
  "module": "lib/index.js",
  "scripts": {
    "build": "rollup --format=cjs --file=dist/aws-transcription-to-vtt.cjs.js lib/index.js",
    "prepublishOnly": "npm run build",
    "test": "tap test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreinstein/aws-transcription-to-vtt.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mreinstein/aws-transcription-to-vtt/issues"
  },
  "homepage": "https://github.com/mreinstein/aws-transcription-to-vtt#readme",
  "devDependencies": {
    "rollup": "^1.2.2",
    "tap": "^14.4.1"
  },
  "keywords": [
    "AWS",
    "Transcribe",
    "JSON",
    "VTT",
    "WebVTT",
    "speech",
    "recognition",
    "format",
    "subtitle",
    "subtitles"
